@article{zalta_twenty-five_1993,
  title = {Twenty-Five Basic Theorems in Situation and World Theory},
  author = {Zalta, Edward N.},
  year = {1993},
  month = aug,
  journal = {Journal of Philosophical Logic},
  volume = {22},
  number = {4},
