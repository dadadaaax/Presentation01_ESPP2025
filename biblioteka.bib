@article{lowney_connecting_2020,
  title = {Connecting {{Twenty-First Century Connectionism}} and {{Wittgenstein}}},
  author = {Lowney, Charles W. and Levy, Simon D. and Meroney, William and Gayler, Ross W.},
  year = {2020},
  month = apr,
  journal = {Philosophia},
  volume = {48},
