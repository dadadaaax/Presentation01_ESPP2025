@misc{lu_shannons_2018,
  title = {From {{Shannon}}'s {{Channel}} to {{Semantic Channel}} via {{New Bayes}}' {{Formulas}} for {{Machine Learning}}},
  author = {Lu, Chenguang},
  year = {2018},
  month = mar,
  number = {arXiv:1803.08979},
